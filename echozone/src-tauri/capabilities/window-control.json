{
  "identifier": "window-control",
  "description": "allow default window control",
  "windows": ["player"],
  "permissions": [
	"core:default",
	"core:window:default",
	"core:window:allow-minimize",
	"core:window:allow-close",
	"core:window:allow-start-dragging",
	"core:window:allow-set-always-on-top"
  ]
}

